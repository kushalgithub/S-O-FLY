@model MvcApplication2.Models.Model_WorLogin

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
 <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
        <meta name="author" content="Coderthemes">

        <link rel="shortcut icon" href='@Url.Content("~/img/favicon_1.ico")'>

        <title>Worker Login</title>

        <link href='@Url.Content("~/css/bootstrap.min.css")' rel="stylesheet" type="text/css" />
        <link href='@Url.Content("~/css/core.css")' rel="stylesheet" type="text/css" />
        <link href='@Url.Content("~/css/components.css")' rel="stylesheet" type="text/css" />
        <link href='@Url.Content("~/css/icons.css")' rel="stylesheet" type="text/css" />
        <link href='@Url.Content("~/css/pages.css")' rel="stylesheet" type="text/css" />
        <link href='@Url.Content("~/css/responsive.css")' rel="stylesheet" type="text/css" />

        <!-- HTML5 Shiv and Respond.js IE8 support of HTML5 elements and media queries -->
